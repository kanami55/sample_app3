<h1>新規投稿</h1>
<% if @list.errors.any? %>
<%=@list.errors.count %>件のエラーが発生しました
<ul>
  <%=@list.errors.full_messages.each do |message| %>
  <li><%= message %></li>
  <% end %>
</ul>
<% end %>
<%= form_with model: @list, url: '/lists', method: :post, local: true do |f| %>

<h1>タイトル</h1>
<%= f.text_field :title %>


<h1>本文</h1>
<%= f.text_area :body %>

<h1>画像</h1>
<%=f.file_field :image, accept: "image/*" %>

<%= f.submit '投稿' %>

<% end %>
